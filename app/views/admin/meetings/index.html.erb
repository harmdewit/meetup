<h1>Listing admin_meetings</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_meetings.each do |admin_meeting| %>
  <tr>
    <td><%= admin_meeting.title %></td>
    <td><%= admin_meeting.description %></td>
    <td><%= link_to 'Show', admin_meeting_path(admin_meeting) %></td>
    <td><%= link_to 'Edit', edit_admin_meeting_path(admin_meeting) %></td>
    <td><%= button_to 'Destroy', admin_meeting_path(admin_meeting), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meeting', new_admin_meeting_path %>
